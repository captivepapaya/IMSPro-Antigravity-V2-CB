{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/wp-api/:path*",
      "destination": "https://lifelikeplants.au/wp-json/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}